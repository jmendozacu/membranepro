<config>
	<modules>
		<WS_ShareThis>
			<version>0.1.0</version>
			<depends>
				<!-- No dependences -->				
			</depends>
		</WS_ShareThis>
	</modules>
	
	<global>
		<models>
	        <sharethis>
	             <class>WS_ShareThis_Model</class>
	        </sharethis>			
		</models>
		<blocks>
            <sharethis><class>WS_ShareThis_Block</class></sharethis>
        </blocks>
		<helpers>
            <sharethis><class>WS_ShareThis_Helper</class></sharethis>			
        </helpers>
	</global>
	
	<frontend>
		<layout>
            <updates>
                <sharethis>
                    <file>sharethis.xml</file>
                </sharethis>
            </updates>
        </layout>
	</frontend>
</config>